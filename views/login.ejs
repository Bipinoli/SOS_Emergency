<% include partials/header %>
      <div id="frm">
      <div class="bs-docs-section">
        <div class="row">
          <div class="col-lg-12">
            <div class="page-header">
              <h1 id="forms">Login</h1>
              <p>Accurate information ensures your own safety. Be as authentic as you can.</p>
            </div>
          </div>
        </div>

        <div class="row">
          <div class="col-lg-6">
            <div class="well bs-component">
              <form class="form-horizontal" id="form" method="POST" action="/login">
                <fieldset>
                  <div id="error">
                  <% if (typeof error != 'undefined') { %>
                  <%= message %>
                  <% } %>
                 </div>
                  <div class="form-group">
                    <div class="col-lg-10">
                      <input type="text" class="default_font input-sm form-control" id="email" name="email" placeholder="Email">
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="col-lg-10"><input type="password" class="default_font input-sm form-control" id="pw" name="pw" placeholder="Password">
                    </div></div><div class="form-group">
                    <div class="col-lg-10 col-lg-offset-2">
                      <button type="submit" id="regsub" class="btn btn-primary">Submit</button>
                    </div>
                  </div>
                </fieldset>
              </form>
            </div>
          </div>
          </div>
                  </div>
<% include partials/footer %>